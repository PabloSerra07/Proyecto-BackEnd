<% if(productos.length > 0){ %>
<div class="table-responsive">
  <table class="table table-dark">
    <tr style="color: rgb(66, 61, 135)">
      <th>Nombre</th>
      <th>Precio</th>
      <th>Url</th>
    </tr>
    <%productos.forEach(function(productos) { %>
    <tr>
      <td><%=productos.nombre%></td>
      <td><%=productos.precio%></td>
      <td><%=productos.url%></td>
    </tr>
    <%});%>
  </table>
  <div>
    <button onclick="window.location.replace('/formulario')"
    type="button" 
    class="btn btn-success"
    >Volver</button>


    
   <!-- <button 
    href="/formulario" 
    type="button" 
    class="btn btn-success"
    >Volver
    </button>  -->
  </div>
</div>
<% } else { %>
<h3 class="alert alert-danger">No se encontraron datos</h3>
<% } %>
